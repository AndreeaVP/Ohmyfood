  },
  "homepage": "https://github.com/daisy2511/Ohmyfood#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "canvas": "^2.11.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "sass": "^1.58.3"
  }
}
