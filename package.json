{
  "name": "ohmyfood",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile:css": "sass --no-source-map css/style.scss:css/style.css --style expanded",
    "prefix:css": "postcss css/style.css -u autoprefixer -b \"last 10 versions\" -o css/style.prefixed.css",
    "compress:css": "sass --no-source-map css/style.prefixed.css:css/style.min.css --style compressed",
    "build": "npm-run-all compile:css prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daisy2511/Ohmyfood.git"
  },
